name=meson
version=0.54.2
release=1
source=(https://github.com/mesonbuild/meson/archive/$version.tar.gz)


build() {
	cd $name-$version

        python3 setup.py build

        python3 setup.py install --root=$PKG
        #cp -rv dest/* /
 
}
